public class Fraction {
  private int made;
  private int attempted;
  
  public fraction (initMade, initAttempted) {
    made = initMade;
    attempted = initAttempted;
  }
  
  public make() {
    made++;
    attempted++;
  }
  
  public miss() {
    attempted++;
  }
  
  public made() {
    return made;
  }
  
  public missed() {
    return attempted - made;
  }
  
  public attempted() {
    return attempted;
  }
}